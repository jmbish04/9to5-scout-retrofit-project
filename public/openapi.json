        },
        "responses": {
          "200": {
            "description": "Generated cover letter",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "cover_letter": { "type": "string" },
                    "html": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/resume": {
      "post": {
        "summary": "Generate AI-optimized resume content",
