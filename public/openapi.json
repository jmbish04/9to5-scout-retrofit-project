                "schema": {
                  "type": "object",
                  "properties": {
                    "summary": { "type": "string" },
                    "experience_bullets": {
                      "type": "array",
                      "items": { "type": "string" }
                    },
                    "skills": {
                      "type": "array", 
                      "items": { "type": "string" }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/email/insights/send": {
