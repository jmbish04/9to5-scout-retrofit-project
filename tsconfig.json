{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "types": [
      "./worker-configuration",
      "node"
    ],
    "lib": [
      "ES2023"
    ]
  },
  "include": [
    "src/**/*.ts",
    "worker-configuration.d.ts"
  ]
}
